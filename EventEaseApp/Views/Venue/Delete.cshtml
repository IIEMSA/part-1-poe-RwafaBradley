@model EventEaseApp.Models.Venue

<h2>Delete Venue</h2>
<div>
    <h4>Are you sure you want to delete this?</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Name</dt>
        <dd class="col-sm-10">@Model.VenueName</dd>
        <dt class="col-sm-2">Location</dt>
        <dd class="col-sm-10">@Model.Locations</dd>
        <dt class="col-sm-2">Capacity</dt>
        <dd class="col-sm-10">@Model.Capacity</dd>
    </dl>
</div>
<form asp-action="Delete">
    <input type="hidden" asp-for="VenueId" />
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a asp-action="Index">Cancel</a>
</form>
